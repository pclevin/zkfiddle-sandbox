<?xml version="1.0" encoding="UTF-8"?>

<zk>
	
    <!-- Defines the application's name -->
    <preference>
        <name>
            org.zkoss.zk.ui.WebApp.name
        </name>
        <value>
            ZK Fiddle Sandboxes
        </value>
    </preference>
	<client-config>
		<debug-js>true</debug-js>
	</client-config>	
	<!-- -->
    <library-property>
        <name>
            org.zkoss.zul.grid.rod
        </name>
        <value>
            true
        </value>
    </library-property>
    <library-property>
        <name>
            org.zkoss.zul.listbox.rod
        </name>
        <value>
            true
        </value>
    </library-property>
    <library-property>
        <name>
            org.zkoss.zul.Button.mold
        </name>
        <value>
            trendy
        </value>
    </library-property>
	<listener>
	    <listener-class>org.zkoss.fiddle.sandbox.ThemeWebInit</listener-class>
	</listener>
	<session-config>
		<timeout-uri />
	</session-config>
</zk>
